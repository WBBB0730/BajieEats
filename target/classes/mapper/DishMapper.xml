<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.szu.Bajie.mapper.DishMapper">

    <resultMap id="BaseResultMap" type="cn.edu.szu.Bajie.entity.Dish">
            <id property="dishId" column="dish_id" jdbcType="INTEGER"/>
            <result property="dishName" column="dish_name" jdbcType="CHAR"/>
            <result property="dishImage" column="dish_image" jdbcType="VARCHAR"/>
            <result property="price" column="price" jdbcType="DECIMAL"/>
            <result property="tags" column="tags" jdbcType="CHAR"/>
            <result property="winId" column="win_id" jdbcType="INTEGER"/>
            <result property="deleteStatus" column="delete_status" jdbcType="INTEGER"/>
            <result property="recommendStatus" column="recommend_status" jdbcType="INTEGER"/>
            <result property="sort" column="sort" jdbcType="INTEGER"/>
            <result property="ingredients" column="ingredients" jdbcType="CHAR"/>
            <result property="score" column="score" jdbcType="DECIMAL"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        dish_id,dish_name,dish_image,
        price,tags,win_id,
        delete_status,recommend_status,sort,
        ingredients,score,create_time,
        update_time
    </sql>
</mapper>
